<div class="row-fluid">
  <div class="main-pane span12">
    <div class="headerpane">
      <h1>
        <span class="record-cell lead">Recycle Bin Configuration</span>
      </h1>
    </div>
    <div class="container-fluid">
      <div>
        <p class="lead">Enable Recycle Bin for below modules</p>
      </div>
      <div class="row-fluid">
        <div style="width: 35%; height: 450px; overflow: auto">
          <table class="table table-bordered table-striped">
          <thead>
            <tr>
              <th width="70%">Modules</th>
              <th width="30%">Enabled?</th>
            </tr>
          </thead>
          <tbody>
            {{#each moduleList}}
            <tr>
              <td>{{this}}</td>
              <td><input type="checkbox"/></td>
            </tr>
            {{/each}}
            </tbody>
          </table>
        </div>
        <div style="margin-top: 30px;">
          <button type="button" class="btn btn-primary">Primary</button>
          <a class="btn btn-invisible btn-link">Cancel</a>
        </div>
      </div>
    </div>
  </div>
</div>